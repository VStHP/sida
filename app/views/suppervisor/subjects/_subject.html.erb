<tr class="row_custom">
  <td class="cell_custom"><%= subject.name %></td>
  <td class="cell_custom"><%= subject.description %></td>
  <td class="cell_custom">
    <%= link_to t("subjects.edit.view_subject"), suppervisor_subject_path(subject) %>
    | <%= link_to t("subjects.edit.edit_subject"), edit_suppervisor_subject_path(subject) %>
    | <%= link_to t("subjects.destroy.delete_link"), [:suppervisor, subject],
        method: :delete, data: { confirm: t("subjects.destroy.confirm") } %>
  </td>
</tr>
